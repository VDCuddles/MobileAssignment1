<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/bg"
tools:context="com.example.aquila.mobileassignment1.MainActivity">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/portal"
        android:id="@+id/imageButton5"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_alignStart="@+id/imageButton3" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/maps"
        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp"
        android:id="@+id/imageButton8"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/about"
        android:id="@+id/imageButton13"
        android:background="#00000000"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/studyoptions"
        android:id="@+id/imageButton4"
        android:background="#00000000"
        android:layout_below="@+id/imageButton2"
        android:layout_alignLeft="@+id/imageButton2"
        android:layout_alignStart="@+id/imageButton2"
        android:layout_marginTop="39dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/youtube"
        android:id="@+id/imageButton10"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/phone"
        android:id="@+id/imageButton7"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/email"
        android:id="@+id/imageButton6"
        android:background="#00000000"
        android:layout_marginBottom="61dp"
        android:layout_above="@+id/imageButton"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/library"
        android:id="@+id/imageButton2"
        android:background="#00000000"
        android:layout_marginTop="59dp"
        android:layout_below="@+id/imageButton13"
        android:layout_toLeftOf="@+id/imageButton10"
        android:layout_toStartOf="@+id/imageButton10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/itsupport"
        android:id="@+id/imageButton3"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton2"
        android:layout_toRightOf="@+id/imageButton10"
        android:layout_toEndOf="@+id/imageButton10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/twitter"
        android:id="@+id/imageButton9"
        android:background="#00000000"
        android:layout_below="@+id/imageButton6"
        android:layout_toLeftOf="@+id/imageButton10"
        android:layout_toStartOf="@+id/imageButton10" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/googleplus"
        android:id="@+id/imageButton11"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_alignRight="@+id/imageButton7"
        android:layout_alignEnd="@+id/imageButton7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/facebook"
        android:id="@+id/imageButton"
        android:background="#00000000"
        android:layout_marginBottom="17dp"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/imageButton7"
        android:layout_toStartOf="@+id/imageButton7" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/linkedin"
        android:id="@+id/imageButton12"
        android:background="#00000000"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_toRightOf="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton11" />

</RelativeLayout>
